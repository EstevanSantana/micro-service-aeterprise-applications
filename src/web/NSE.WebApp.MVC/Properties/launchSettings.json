{
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iis": {
      "applicationUrl": "http://localhost/NSE.WebApp.MVC",
      "sslPort": 0
    },
    "iisExpress": {
      "applicationUrl": "http://localhost:37966",
      "sslPort": 44356
    }
